        <script src="${request.static_url('startingout:static/js/isotope.js')}"></script>
        <script>
            var $container = jQuery('.isotope-content');

            var $buttons = jQuery($container).isotope({
                // options
                itemSelector: '.bookmarks-button',
                layoutMode: 'fitRows',

            });


            // Filter content based on first category
            jQuery('.filter-list button').on('click', function () {
                if (jQuery(this).hasClass('isotope_active_filter')) {
                    jQuery(this).removeClass('isotope_active_filter');
                    $container.isotope({
                        filter: '*',
                        getSortData: {name: '.bookmarks-button'},
                        sortBy: 'name'
                    })

                } else {
                    jQuery('.filter-list button').removeClass('isotope_active_filter');
                    var filterValue = jQuery(this).attr('data-filter');
                    $container.isotope({
                        filter: filterValue,
                        getSortData: {name: '.bookmarks-button'},
                        sortBy: 'name'
                    });
                    jQuery(this).addClass('isotope_active_filter');
                }
            });
        </script>
